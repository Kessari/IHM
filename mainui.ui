   <property name="styleSheet">
    <string notr="true">
#centralwidget {
    background: transparent;
}

/* شريط التوهج تحت الأعلى */
#frame_heroAccent {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 transparent,
                                stop:0.25 rgba(123, 47, 247, 0.6),
                                stop:0.5 rgba(241, 7, 163, 0.8),
                                stop:0.75 rgba(123, 47, 247, 0.6),
                                stop:1 transparent);
    border-radius: 4px;
}

/* البادج - خلفية شفافة */
#label_heroBadge {
    background: transparent;
    color: #7B2FF7;
    border-radius: 999px;
    padding: 6px 18px;
    font-size: 11px;
    font-weight: 600;
    border: 1.5px solid rgba(123, 47, 247, 0.2);
}

/* عنوان الهيرو - بلونين فقط */
#label_heroTitle {
    font-size: 26px;
    font-weight: 800;
    color: #7B2FF7;
    letter-spacing: -0.5px;
    padding: 5px 0;
}

/* وصف الهيرو */
#label_heroSubtitle {
    color: #6B6685;
    font-size: 13px;
    line-height: 1.4;
    opacity: 0.9;
}

/* الكارد الزجاجية */
#frame_card {
    background: rgba(255, 255, 255, 0.92);
    border-radius: 28px;
    border: 1px solid rgba(255, 255, 255, 0.4);
    box-shadow: 0 8px 32px rgba(123, 47, 247, 0.08),
                0 2px 4px rgba(123, 47, 247, 0.04);
}

/* حقول الإدخال */
QLineEdit {
    background-color: rgba(255, 255, 255, 0.9);
    border-radius: 18px;
    border: 1.5px solid rgba(155, 132, 222, 0.2);
    padding: 10px 20px;
    font-size: 13px;
    color: #1F1638;
    selection-background-color: rgba(123, 47, 247, 0.2);
    selection-color: #1F1638;
}
QLineEdit:hover {
    border: 1.5px solid rgba(123, 47, 247, 0.3);
    background-color: rgba(255, 255, 255, 0.95);
}
QLineEdit:focus {
    border: 1.5px solid #7B2FF7;
    background-color: white;
    box-shadow: 0 0 0 3px rgba(123, 47, 247, 0.1);
}

/* تذكّرني */
QCheckBox {
    color: #6B6685;
    font-size: 12px;
    spacing: 8px;
}
QCheckBox::indicator {
    width: 16px;
    height: 16px;
    border-radius: 4px;
    border: 1.5px solid rgba(123, 47, 247, 0.3);
    background: white;
}
QCheckBox::indicator:checked {
    background-color: #7B2FF7;
    border: 1.5px solid #7B2FF7;
}

/* زر نسيت كلمة المرور */
#pushButton_forgot {
    border: none;
    background: transparent;
    color: #7B2FF7;
    font-size: 12px;
    font-weight: 500;
    padding: 4px 8px;
    border-radius: 6px;
}
#pushButton_forgot:hover {
    background: rgba(123, 47, 247, 0.05);
}

/* رسائل الخطأ */
#errorEmail, #errorPassword {
    color: #FF3B30;
    font-size: 11px;
    padding-left: 4px;
    font-weight: 500;
}

/* زر Sign in الأساسي */
#pushButton_login {
    border: none;
    border-radius: 20px;
    padding: 12px 24px;
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #7B2FF7,
                                stop:1 #F107A3);
    color: white;
    font-size: 14px;
    font-weight: 600;
    letter-spacing: 0.3px;
}
#pushButton_login:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:0,
                                stop:0 #8B3DFF,
                                stop:1 #FF1AA3);
}

/* زر Create account */
#pushButton_createAccount {
    border-radius: 20px;
    padding: 10px 24px;
    background: rgba(255, 255, 255, 0.7);
    border: 1.5px solid rgba(123, 47, 247, 0.15);
    color: #1F1638;
    font-size: 13px;
    font-weight: 600;
}
#pushButton_createAccount:hover {
    background: rgba(255, 255, 255, 0.9);
    border: 1.5px solid rgba(123, 47, 247, 0.3);
}

/* أزرار السوشيال */
#pushButton_facebook, #pushButton_google {
    border-radius: 18px;
    padding: 10px 16px;
    background: rgba(255, 255, 255, 0.7);
    border: 1.5px solid rgba(255, 255, 255, 0.4);
    font-size: 12px;
    font-weight: 600;
}
#pushButton_facebook {
    color: #1877F2;
    border-color: rgba(24, 119, 242, 0.25);
}
#pushButton_facebook:hover {
    background: rgba(24, 119, 242, 0.05);
    border-color: rgba(24, 119, 242, 0.4);
}
#pushButton_google {
    color: #EA4335;
    border-color: rgba(234, 67, 53, 0.25);
}
#pushButton_google:hover {
    background: rgba(234, 67, 53, 0.05);
    border-color: rgba(234, 67, 53, 0.4);
}

/* نص إضافي */
#label_or {
    color: #8B86AA;
    font-size: 12px;
    letter-spacing: 1px;
    opacity: 0.7;
}

/* تسميات الحقول */
#label_email, #label_password {
    font-size: 12px;
    font-weight: 600;
    color: #1F1638;
    padding-left: 4px;
}
